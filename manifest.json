{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "icons": {},
    "content_scripts": [
        {
            "matches": [
                "*://localhost:*/*",
                "*://127.0.0.1:*/*"
            ],
            "js": [
                "reload.js"
            ]
        }
    ],
    "background": {
        
    },
    "permissions": [
        "tabs"
    ]
}