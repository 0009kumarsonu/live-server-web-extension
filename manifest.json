{
    "manifest_version": 2,
    "name": "Live Server Web Extension",
    "version": "0.0.1",
    "description": "Live Server Web Extension",
    "icons": {},
    "content_scripts": [
        {
            "matches": [
                "*://localhost:*/*",
                "*://127.0.0.1:*/*"
            ],
            "js": [
                "reload.js"
            ]
        }
    ],
    "background": {
        
    },
    "permissions": [
        "tabs"
    ]
}